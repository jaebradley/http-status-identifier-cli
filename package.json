{
  "name": "http-status-identifier-cli",
  "version": "0.0.0-development",
  "description": "Identify http status from the command line",
  "bin": {
    "hs": "build/index.js"
  },
  "scripts": {
    "codecov": "codecov",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "build": "babel src/ -d build/ --delete-dir-on-start --copy-files",
    "build:prod": "BABEL_ENV=production npm run build",
    "lint": "eslint --ext .js .",
    "test": "jest src",
    "prepublishOnly": "npm run build:prod",
    "semantic-commit": "commit",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jaebradley/http-status-identifier-cli.git"
  },
  "keywords": [
    "http",
    "http status codes",
    "http cli",
    "http status code cli"
  ],
  "author": "jae.b.bradley@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jaebradley/http-status-identifier-cli/issues"
  },
  "homepage": "https://github.com/jaebradley/http-status-identifier-cli#readme",
  "dependencies": {
    "cli-table2": "^0.2.0",
    "commander": "^2.15.1",
    "http-status-identifier": "^2.0.1",
    "node-emoji": "^1.8.1",
    "open": "0.0.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.46",
    "@babel/core": "^7.0.0-beta.46",
    "@babel/preset-env": "^7.0.0-beta.46",
    "@commitlint/cli": "^6.1.3",
    "@commitlint/config-angular": "^6.1.3",
    "@commitlint/prompt": "^6.1.3",
    "@commitlint/prompt-cli": "^6.1.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.0.0",
    "babel-preset-minify": "^0.4.1",
    "codecov": "^3.0.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.9.0",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "semantic-release": "^15.4.1",
    "travis-deploy-once": "^5.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": true
  }
}
